{{#each category}}
  <h1>{{this.name}}</h1>
  <img class="img-fluid" src="{{this.image}}" alt="">

  <div class="container-fluid">
    <div class="row row-cols-3 g-3">
      {{#each ../products}}
      <br> </br>
        {{#if services}}
          {{#each services}}
            <div class="col">
              <div class="card">
                {{#if this.image}}
                  <img src="{{this.image}}" class="card-img-top" alt="Card image cap">
                {{/if}}
                <div class="card-body">
                  <h5 class="card-title"><a href="{{link}}" class="card-link">{{this.name}}</a></h5>
                  <p class="card-text">{{this.description}}</p>
                  <ul class="list-group list-group-flush">
                    <li class="list-group-item">${{this.price}}</li>
                  </ul>
                </div>
              </div>
            </div>
          {{/each}}
        {{/if}}
      {{/each}}
    </div>
  </div>
{{/each}}
