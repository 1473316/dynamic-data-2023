{{#each cartProducts}}
<div class="cart-item">
    <div class="item-image">
        <img class="img-fluid" src="{{this.image}}" alt="">
    </div>
    <div class="item-info">
        <ul>
            <li><strong>Name:</strong> {{this.name}}</li>
            <li><strong>Price:</strong> ${{this.price}}</li>
            <li><strong>Description:</strong> {{this.description}}</li>
        </ul>
    </div>
</div>
{{/each}}


<form action="/check-out" method="get">
    <button type="submit">Check Out</button>
    </form>